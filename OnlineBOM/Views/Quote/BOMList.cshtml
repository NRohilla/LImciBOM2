            $('#DrpDwnConsummable_SolventPrice').html(GetPriceConsummableSolvent);
            var GetQtyOfConsummableSolvent = $(this).find(':selected').attr('data-SolventQty');
            $('#DrpDwnConsummable_SolventQty').html(GetQtyOfConsummableSolvent);
            var GetFinalPriceSolvent = parseFloat(GetPriceConsummableSolvent) * parseFloat(GetQtyOfConsummableSolvent);
            $('#DrpDwnConsummable_SolventFinalPrice').html(GetFinalPriceSolvent);

            var GetcleanerID = $(this).find(':selected').attr('data-cleanerID');
