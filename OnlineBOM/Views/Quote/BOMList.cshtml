            var GetFinalPrice = parseFloat(GetPriceConsummable_Additional) * parseFloat(GetQtyOfConsummable_Additional);
            $('#DrpDwnConsummable_Additional_FinalPrice').html(GetFinalPrice);


            var GetSolventID = $(this).find(':selected').attr('data-SolventID');
            $('#DrpDwnConsummable_Additional_SolventItemID').html(GetSolventID);
