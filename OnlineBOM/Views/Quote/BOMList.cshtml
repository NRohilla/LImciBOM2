            $('#DrpDwnConsummable_CleanerItemDesc').html(GetcleanerDesc);
            var GetPriceConsummableCleaner = $(this).find(':selected').attr('data-CleanerPrice');
            $('#DrpDwnConsummable_CleanerPrice').html(GetPriceConsummableCleaner);
            var GetQtyOfConsummableCleaner = $(this).find(':selected').attr('data-CleanerQty');
            $('#DrpDwnConsummable_CleanerQty').html(GetQtyOfConsummableCleaner);
            var GetFinalPriceCleaner = parseFloat(GetPriceConsummableCleaner) * parseFloat(GetQtyOfConsummableCleaner);
            $('#DrpDwnConsummable_CleanerFinalPrice').html(GetFinalPriceCleaner);
        });

       
        $('#DrpDwnConsummable_Additional').change(function () {
            var GetItemIDOfCOns = $(this).find(':selected').val();
            $('#DrpDwnConsummable_Additional_ItemID').html(GetItemIDOfCOns);
            var GetPriceConsummable_Additional = $(this).find(':selected').attr('data-PriceOfConsummable_Additional');
            $('#DrpDwnConsummable_Additional_Price').html(GetPriceConsummable_Additional);
