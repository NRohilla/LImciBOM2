            $('#DrpDwnConsummable_Additional_SolventFinalPrice').html(GetFinalPriceSolvent);

            var GetcleanerID = $(this).find(':selected').attr('data-cleanerID');
            $('#DrpDwnConsummable_Additional_CleanerItemID').html(GetcleanerID);
            var GetcleanerDesc = $(this).find(':selected').attr('data-cleanerDesc');
            $('#DrpDwnConsummable_Additional_CleanerItemDesc').html(GetcleanerDesc);
            var GetPriceConsummable_AdditionalCleaner = $(this).find(':selected').attr('data-CleanerPrice');
            $('#DrpDwnConsummable_Additional_CleanerPrice').html(GetPriceConsummable_AdditionalCleaner);
            var GetQtyOfConsummable_AdditionalCleaner = $(this).find(':selected').attr('data-CleanerQty');
            $('#DrpDwnConsummable_Additional_CleanerQty').html(GetQtyOfConsummable_AdditionalCleaner);
            var GetFinalPriceCleaner = parseFloat(GetPriceConsummable_AdditionalCleaner) * parseFloat(GetQtyOfConsummable_AdditionalCleaner);
            $('#DrpDwnConsummable_Additional_CleanerFinalPrice').html(GetFinalPriceCleaner);
        });

        //Change event of the quantity of the device/printers
        $("td[data-type='Device'").change(function () {
            //Loop throgh the DDL Options containing all the printheads compatible
            $('option[data-attr="Option_DrpDwnConsummable"]').each(function (index) {
                //Create the list of print heads
                var GetPrintHead = $(this).attr('data-CompatiblePrintHeads').split(';');
                var HideThisOption = true;

                for (i = 0; i < GetPrintHead.length; i++) {

                    if (GetPrintHead[i].trim().length > 0) {
                        //Check the Qty of this PrintHead
                        var GetQtyOfThisPrintHd = parseFloat($("td[data-PrintHeadID='" + GetPrintHead[i] + "'").html());
                        if (GetQtyOfThisPrintHd > 0) {
                            HideThisOption = false;
                        }
                    }
                }

                if (HideThisOption) {
                    if ($(this).parents('span').html() == undefined) {
                        $(this).wrap('<span/>');
                        debugger;
                    }
                } else {
                    if ($(this).parents('span').html() != undefined) {
                        $(this).unwrap();
                        debugger;
                    }
                }
            });
        });
    });
</script>