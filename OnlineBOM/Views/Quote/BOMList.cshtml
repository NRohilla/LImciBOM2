            var DataAttr = $(this).attr('data-id');
            $("tr[id='" + DataAttr + "']").each(function () {
                $(this).toggle();
            });
        });

        $('#DrpDwnConsummable').change(function () {
            var GetItemIDOfCOns = $(this).find(':selected').val();
